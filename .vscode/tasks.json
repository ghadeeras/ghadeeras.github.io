{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "./tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "tsc: build"
		},
		{
			"type": "deno",
			"command": "lint",
			"problemMatcher": [
				"$deno-lint"
			],
			"group": "test",
			"label": "deno: lint",
			"args": ["src"]
		}
	]
}